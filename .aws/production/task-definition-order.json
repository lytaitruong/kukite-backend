{
  "containerDefinitions": [
    {
      "name": "kukite-production-order-api",
      "image": "044043080677.dkr.ecr.ap-southeast-1.amazonaws.com/kukite-production-order-api",
      "cpu": 1024,
      "memory": 2048,
      "memoryReservation": 2048,
      "portMappings": [
        {
          "name": "kukite-production-order-api-3008-tcp",
          "containerPort": 3008,
          "hostPort": 3008,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::kukite-ecs-configuration/production/order-api/.env",
          "type": "s3"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/kukite-production-task-definition",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "family": "kukite-production-task-definition",
  "taskRoleArn": "arn:aws:iam::044043080677:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::044043080677:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "ephemeralStorage": {
    "sizeInGiB": 20
  },
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": [
    {
      "key": "environment",
      "value": "production"
    }
  ]
}
